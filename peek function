int peek(struct node * top , int pos)
{
    struct node * ptr = top;
    for(int i = 0 ; (i<pos-1 && ptr!=NULL ); i++)
    {
        ptr = ptr->next;
    }
    if(ptr!=NULL)
    {
        return ptr->data;
    }
    else
    {
        return -1;
    }
}

int peek(struct node * sp , int i)
{
    if(sp->top-i-1<0)
    {
        return -1;
    }
    else
    {
        return sp->arr[top-i-1];
    }
    
}
  
